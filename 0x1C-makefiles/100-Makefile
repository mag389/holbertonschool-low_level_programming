# -*- MakeFile -*-
CC=gcc
SRC=main.c holberton.c
OBJ=$(patsubst %.c,%.o,$(SRC))
NAME=holberton
CFLAGS= -Wall -Werror -Wextra -pedantic
.PHONY: clean oclean flcean re

all: $(OBJ) m.h
	$(CC) $(OBJ) -o $(NAME)

clean:
	$(RM) -f *~ $(NAME)

oclean:
	$(RM) -f $(OBJ)

fclean: clean oclean

re: oclean all
